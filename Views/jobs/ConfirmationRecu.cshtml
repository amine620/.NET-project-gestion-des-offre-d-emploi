
@{
    ViewBag.Title = "ConfirmationRecu";
}

<h2>ConfirmationRecu</h2>

<table id="example" class="table table-striped table-bordered" style="width:100%;margin-left: -75px;margin-top: 85px;">
    <tr>
        <th>
            @Html.Label("Message Confirmation")
        </th>
        <th>
            @Html.Label("Date Recu")
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <strong>@Html.DisplayFor(modelItem => item.Message)</strong>
            </td>
            <td>
                <strong>@Html.DisplayFor(modelItem => item.Date)</strong>
            </td>
        </tr>
    }

</table>
<script>
    $(document).ready(function () {
        $('#example').DataTable();

</script>