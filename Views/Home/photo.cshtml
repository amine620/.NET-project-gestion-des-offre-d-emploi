@using Microsoft.AspNet.Identity
@using wafabank.Models


@{
    //Layout = null;
}

@using (Html.BeginForm("photo", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    var db = new ApplicationDbContext();
    var profil = db.Profils;
    @Html.AntiForgeryToken()
    foreach (var item in profil)
    {
<img  src="~/upload/@Html.DisplayFor(m=>item.photo)" class="thumbnail" style="width:48px;height:48px;"/>

    

}
<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <input type="submit" value="Create" class="btn btn-success" />
    </div>
</div>
}
